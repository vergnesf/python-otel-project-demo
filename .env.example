# Environment Variables Example File
# Copy this file to .env and replace the placeholder values with your actual configuration.

# Global Settings
# DOCKER_REGISTRY=your_docker_registry_url
IMG_PYTHON_BASE=python:3.14-slim

# Grafana Service Account Token
GRAFANA_SERVICE_ACCOUNT_TOKEN=replace_with_your_token

# Docker image variables used by docker-compose files. Copy to .env to override.
# Format: DOCKER_IMG_<SERVICE>=<image:tag>
DOCKER_IMG_GRAFANA=grafana/grafana:12.2
DOCKER_IMG_OTEL_COLLECTOR=otel/opentelemetry-collector-contrib:0.139.0
DOCKER_IMG_LOKI=grafana/loki:3.5.8
DOCKER_IMG_TEMPO=grafana/tempo:2.9.0
DOCKER_IMG_MIMIR=grafana/mimir:3.0.0
DOCKER_IMG_GRAFANA_MCP=mcp/grafana:latest
DOCKER_IMG_OLLAMA=ollama/ollama:latest
DOCKER_IMG_POSTGRES=postgres:18
DOCKER_IMG_ADMINER=adminer:5.4.1
DOCKER_IMG_KAFKA=confluentinc/cp-kafka:8.1.0
DOCKER_IMG_AKHQ=tchiotludo/akhq:0.26.0

